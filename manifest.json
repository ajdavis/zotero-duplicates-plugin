{
	"manifest_version": 2,
	"name": "Find Duplicate Items",
	"version": "1.1",
	"description": "Finds duplicate parent items by PDF content and title similarity, tags them for cleanup",
	"applications": {
		"zotero": {
			"id": "find-duplicates@example.com",
			"update_url": "https://raw.githubusercontent.com/ajdavis/zotero-duplicates-plugin/main/update.json",
			"strict_min_version": "6.999",
			"strict_max_version": "8.99.*"
		}
	}
}
